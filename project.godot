[application]

config/name="BITSHIFT OVERDRIVE 1.2"
run/main_scene="res://scenes/TitleScreen.tscn"
boot_splash/image="res://assets/splash.png"
config/icon="res://icon.png"

[autoload]

UserPreferences="*res://scripts/global/UserPreferences.gd"
Sounds="*res://scenes/global/Sounds.tscn"
Tracks="*res://scenes/global/Tracks.tscn"
Active="*res://scripts/global/Active.gd"
ChartManager="*res://scripts/global/ChartManager.gd"
ScoreManager="*res://scripts/global/ScoreManager.gd"
SoundManager="*res://scripts/global/SoundManager.gd"
TrophyManager="*res://scripts/global/TrophyManager.gd"
ss="*res://addons/godot_sprite_shader/ss.gd"

[debug]

settings/crash_handler/message="BITSHIFT OVERDRIVE has encountered a fatal error."
settings/fps/force_fps=60

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false
mouse_cursor/custom_image="res://assets/sprites/cursor-slim.png"
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot_sprite_shader/plugin.cfg" )

[gui]

timers/tooltip_delay_sec=0.35

